    <groupId>tech.beshu.ror</groupId>
    <artifactId>ror-custom-java-serializer</artifactId>
    <version>1.0.0</version>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>8</source>
                    <target>8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <dependencies>
        <dependency>
            <groupId>tech.beshu.ror</groupId>
            <artifactId>audit_2.13</artifactId>
            <version>1.18.9</version>
            <!-- <version>ROR_VERSION</version> -->
        </dependency>
        <dependency>
            <groupId>org.scala-lang</groupId>
            <artifactId>scala-library</artifactId>
            <version>2.13.1</version>
        </dependency>
    </dependencies>

</project>